<Application x:Class="FUNERALMVVM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FUNERALMVVM"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             StartupUri="/View/Pages/IssuePage.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source = "View/Dictionary/MenuButtonTheme.xaml"/>
                <ResourceDictionary Source="View/Dictionary/ButtonTheme.xaml"/>
                <ResourceDictionary Source="View/Dictionary/ComboBoxTheme.xaml"/>
                <ResourceDictionary Source="View/Dictionary/LabelTheme.xaml"/>
                <ResourceDictionary Source="View/Dictionary/TableDictionary.xaml"/>
                <ui:ThemesDictionary Theme="Light" />
                <ui:ControlsDictionary />
                <ResourceDictionary Source="pack://application:,,,/Wpf.Ui;component/Styles/Wpf.Ui.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
